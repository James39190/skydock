<script type="text/javascript">
/*<![CDATA[*/
jQuery(window).load(function() {
$('#_link0').click(function() { 
$('#_block0').toggle(200, function() { 
var vis=$('#_block0:hidden').length; 
if (vis > 0) {
$('#_link0').html('(+)');
 $('#_link0').attr('title','Show');
} else { 
 $('#_link0').html('(-)');
$('#_link0').attr('title','hide');
}}) 
 });
});
/*]]>*/
</script>
